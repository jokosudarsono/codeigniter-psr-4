{
	"description": "The Custom Autoload CodeIgniter 3 framework",
	"name": "psr4igniter",
	"type": "project",
	"license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\Models\\": "application/models/",
            "App\\Libraries\\": "application/libraries/"
        },
        "classmap": ["application/core"]
    },
	"require": {
		"php": ">=5.2.4"
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.*",
		"phpunit/phpunit": "4.* || 5.*"
	}
}
